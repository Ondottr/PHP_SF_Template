{
  "authors": [
    {
      "name": "Dmytro Dyvulskyi",
      "role": "CEO & Founder and Lead Developer of Nations Original",
      "email": "eklerk397@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "App/",
      "App\\View\\": "templates/",
      "Doctrine\\": "Platform/CustomRepositories/Doctrine"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "symfony/thanks": true
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.*"
    }
  },
  "license": "ISC",
  "minimum-stability": "stable",
  "name": "nations-original/php-simple-framework-template",
  "prefer-stable": true,
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "require": {
    "php": "8.3.*",
    "ext-amqp": "*",
    "ext-apcu": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-http": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-memcached": "*",
    "ext-posix": "*",
    "ext-redis": "*",
    "ext-yaml": "*",
    "ext-zend-opcache": "*",
    "ext-zlib": "*",
    "composer/package-versions-deprecated": "1.11.99.4",
    "nelmio/api-doc-bundle": "^4.16",
    "php-amqplib/php-amqplib": "^3.6",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.24",
    "predis/predis": "^1.1",
    "psr/simple-cache": "^3.0",
    "symfony/amqp-messenger": "7.*",
    "symfony/asset": "7.*",
    "symfony/console": "7.*",
    "symfony/dotenv": "7.*",
    "symfony/expression-language": "7.*",
    "symfony/flex": "^1.17|^2",
    "symfony/framework-bundle": "7.*",
    "symfony/http-client": "7.*",
    "symfony/intl": "7.*",
    "symfony/messenger": "7.*",
    "symfony/mime": "7.*",
    "symfony/process": "7.*",
    "symfony/property-access": "7.*",
    "symfony/property-info": "7.*",
    "symfony/proxy-manager-bridge": "*",
    "symfony/runtime": "7.*",
    "symfony/serializer": "7.*",
    "symfony/string": "7.*",
    "symfony/twig-bundle": "7.*",
    "symfony/web-link": "7.*",
    "symfony/yaml": "7.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "phpunit/php-code-coverage": "10.*",
    "phpunit/phpunit": "10.*",
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "7.*",
    "symfony/css-selector": "7.*",
    "symfony/debug-bundle": "7.*",
    "symfony/maker-bundle": "^1.43",
    "symfony/phpunit-bridge": "^6.2",
    "symfony/stopwatch": "7.*",
    "symfony/thanks": "^1.2",
    "symfony/web-profiler-bundle": "7.*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "type": "project"
}
